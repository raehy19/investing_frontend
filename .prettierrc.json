{
  "singleQuote": true,
  "trailingComma": "all",
  "semi": true,
  "useTabs": true,
  "tabWidth": 2,
  "printWidth": 100,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "auto"
}
